	font-style: normal;
	src: url(data:font/ttf;base64,) format('truetype');
}
.uni-flex {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
}
.uni-flex-item {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.uni-row {
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
}
.uni-column {
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.uni-link{
	color:#576B95;
	font-size:26rpx;
}

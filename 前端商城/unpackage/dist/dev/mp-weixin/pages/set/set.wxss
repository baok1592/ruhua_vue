page {
  background: #f8f8f8;
  padding-top: 10px;
}
.list {
  background-color: #fff;
  border-bottom: 1px solid #F8F8F8;
  padding: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 15px;
}
.list .list_l {
    height: 40px;
    line-height: 40px;
}
.list .list_r {
    height: 40px;
    line-height: 40px;
    color: #999999;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.list .list_r .list_r_1 ._img {
      height: 40px;
      width: 40px;
      border-radius: 40px;
}
.list .list_r .list_r_1 input {
      text-align: right;
      font-size: 13px;
      margin-top: 11px;
}
.list .list_r .list_r_2 {
      padding-left: 5px;
      margin-top: -1px;
      font-size: 13px;
}
.list .list_r .uni-list-cell-pd {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
}
.list .list_r .nan {
      padding-right: 10px;
}
.nicheng .tan_nc {
  background-color: #fff;
  z-index: 9999;
  position: fixed;
  top: 130px;
  left: 20%;
  width: 60%;
}
.nicheng .tan_nc .nc_in {
    padding: 30px 20px;
}
.nicheng .tan_nc .nc_btn {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    font-size: 14px;
    border-top: 1px solid #F4F4F4;
}
.nicheng .tan_nc .nc_btn .btn_l {
      width: 50%;
      text-align: center;
      padding: 15px 0;
      border-right: 1px solid #F4F4F4;
}
.nicheng .tan_nc .nc_btn .btn_r {
      width: 50%;
      text-align: center;
      padding: 15px 0;
}
.nicheng .container {
  background-color: #000000;
  position: fixed;
  top: 0;
  opacity: 0.6;
  width: 100%;
  height: 100%;
  z-index: 999;
}
.list-cell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: baseline;
  -webkit-align-items: baseline;
          align-items: baseline;
  padding: 20rpx 30rpx;
  line-height: 60rpx;
  position: relative;
  background: #fff;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.list-cell.log-out-btn {
    margin-top: 40rpx;
}
.list-cell.log-out-btn .cell-tit {
      color: #fa436a;
      text-align: center;
      margin-right: 0;
}
.list-cell.cell-hover {
    background: #fafafa;
}
.list-cell.b-b:after {
    left: 30rpx;
}
.list-cell.m-t {
    margin-top: 16rpx;
}
.list-cell .cell-more {
    -webkit-align-self: baseline;
            align-self: baseline;
    font-size: 32rpx;
    color: #909399;
    margin-left: 10rpx;
}
.list-cell .cell-tit {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    font-size: 30rpx;
    color: #303133;
    margin-right: 10rpx;
}
.list-cell .cell-tip {
    font-size: 28rpx;
    color: #909399;
}
.list-cell switch {
    -webkit-transform: translateX(16rpx) scale(0.84);
            transform: translateX(16rpx) scale(0.84);
}

